{
  "name": "@poesy-liang-blog/app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/start-dev.js",
        "cwd": "apps/app"
      }
    },
    "dev:direct": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev",
        "cwd": "apps/app"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next build",
        "cwd": "apps/app"
      },
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/.next"]
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start",
        "cwd": "apps/app"
      },
      "dependsOn": ["build"]
    }
  }
}
